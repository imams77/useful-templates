# MongoDB service environment variables
# Copy to `.env` in this folder (or set in CI) and fill secure values. Do not commit `.env`.

# Admin user for MongoDB
MONGO_INITDB_ROOT_USERNAME=admin
MONGO_INITDB_ROOT_PASSWORD=change_me

# Mongo-Express (UI) credentials
ME_CONFIG_MONGODB_ADMINUSERNAME=admin
ME_CONFIG_MONGODB_ADMINPASSWORD=change_me
ME_CONFIG_BASICAUTH_USERNAME=change_me
ME_CONFIG_BASICAUTH_PASSWORD=change_me
